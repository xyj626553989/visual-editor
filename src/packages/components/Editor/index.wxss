.editor {
  flex: 1;
  box-sizing: border-box;
  padding-top: 60px;
}
.editor-container {
  background-color: #fff;
  margin: 50px auto;
  position: relative;
}
.editor-container .component-item {
  position: absolute;
}
.editor-container .component-item.component-item-foucs::after {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  content: "";
  border: 1px solid #4bfde5;
}
